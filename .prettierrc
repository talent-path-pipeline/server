{
  "trailingComma": "all",
  "eslintIntegration": true,
  "singleQuote": true,
  "printWidth": 90,
  "tabWidth": 2,
  "semi": true,
  "htmlWhitespaceSensitivity": "css",
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "endOfLine": "crlf",
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "quoteProps": "as-needed",
  "useTabs": false
}
